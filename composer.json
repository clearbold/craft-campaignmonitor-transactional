{
    "name": "clearbold/craft-campaignmonitor-transactional",
    "description": "Campaign Monitor Transactional email adapter for Craft CMS.",
    "version": "0.0.1",
    "type": "craft-plugin",
    "homepage": "https://github.com/clearbold/craft-campaignmonitor-transactional",
    "license": "Craft",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "email",
        "campaignmonitor"
    ],
    "require": {
        "craftcms/cms": "^3.0.1",
        "campaignmonitor/createsend-php": "^5.1.3",
        "zbateson/mail-mime-parser": "^0.4.9"
    },
    "autoload": {
        "psr-4": {
